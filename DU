for z in range(1, 101):

    if (z % 2 == 1):
        print("Číslo je nepárne")
    else:
        print("Číslo je párne") 

    if (z % 10 == 0):
        print("Číslo je desiatka")

    prvocislo = True 


    for y in range(2,z):


        if (z % y == 0) == True:
            je_prvocislo = False


    if (prvocislo == True):
        print("Číslo je prvočíslo")
    else:
        print("Číslo nie je prvočíslo")
